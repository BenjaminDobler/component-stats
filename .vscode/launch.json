{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/dist/apps/vscode-extension"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/apps/vscode-extension/**/*.js"
      ],
      "preLaunchTask": "npm: build"
    }
  ]
}
