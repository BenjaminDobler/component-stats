{
  "name": "@component-stats/workspace",
  "version": "1.0.0",
  "description": "Nx workspace for Angular component analysis tools",
  "scripts": {
    "build": "nx build component-stats",
    "build:all": "nx run-many --target=build --all",
    "build:demo": "nx build demo",
    "serve:demo": "nx serve demo",
    "test": "nx test component-stats",
    "lint": "nx run-many --target=lint --all",
    "publish:prepare": "nx build component-stats && cp dist/libs/component-stats/package-dist.json dist/libs/component-stats/package.json",
    "publish:npm": "npm run publish:prepare && cd dist/libs/component-stats && npm publish"
  },
  "private": true,
  "keywords": [
    "angular",
    "component",
    "analysis",
    "stats",
    "parser"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^20.3.14",
    "@angular/compiler": "^20.3.14",
    "@angular/compiler-cli": "^20.3.14",
    "@angular/core": "^20.3.14",
    "@angular/forms": "^20.3.14",
    "@angular/language-service": "^20.3.14",
    "@angular/platform-browser": "^20.3.14",
    "@angular/platform-browser-dynamic": "^20.3.14",
    "@angular/router": "^20.3.14",
    "glob": "^11.0.0",
    "rxjs": "^7.8.2",
    "ts-morph": "^21.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.6.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.12",
    "@angular-devkit/core": "^20.3.12",
