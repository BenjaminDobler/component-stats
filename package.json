    "@angular/build": "^20.3.12",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "^22.1.2",
    "@nx/devkit": "^22.1.2",
    "@nx/eslint": "22.1.2",
    "@nx/eslint-plugin": "22.1.2",
    "@nx/js": "^22.1.2",
    "@nx/web": "22.1.2",
    "@schematics/angular": "^20.3.12",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "^22.0.0",
    "@typescript-eslint/utils": "^8.40.0",
    "angular-eslint": "^20.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
